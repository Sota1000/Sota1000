<script>
		function youTubes_makeDynamic() {
			var $ytIframes = $('iframe[src*="dailymotion.com"]');
			$ytIframes.each(function (i,e) {
				var $ytFrame = $(e);
				var ytKey; var tmp = $ytFrame.attr('src').split(/\//); tmp = tmp[tmp.length - 1]; tmp = tmp.split('?'); ytKey = tmp[0];
				var $ytLoader = $('<div class="ytLoader">');
				$ytLoader.append($('<img loading="lazy" class="cover" src="images/community/dailymotion-'+ytKey+'.jpg" width="629" height="354" />'));
				$ytLoader.append($('<img loading="lazy" class="playBtn" src="images/community/play_button.png" width="94" height="93" />'));
				$ytLoader.data('$ytFrame',$ytFrame);
				$ytFrame.replaceWith($ytLoader);
				$ytLoader.click(function () {
					var $ytFrame = $ytLoader.data('$ytFrame');
					$ytFrame.attr('src',$ytFrame.attr('src')+'&autoplay=1');
					$ytLoader.replaceWith($ytFrame);
				});
			});
		};
		$(document).ready(function () {youTubes_makeDynamic()});
	</script>

